    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-bdd": "^2.1.1",
    "mustache": "4.2.0",
    "expect.js": "^0.3.1",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0",
    "prettier": "2.4.1",
    "typescript": "^4.4.3",
